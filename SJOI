#include <stdio.h>
int a,b,n=0;
char SJOI[20][20],S[4]={'S','J','O','I'};
void F(int i,int j,int c){
	if(SJOI[i][j]==S[c]&&S[c]!='I'){
		if(i+1<a){
			if(j-1>=0){F(i+1,j-1,c+1);}
			else if(j+1<b){F(i+1,j+1,c+1);}
			else{F(i+1,j,c+1);}
		}
		else if(i-1>=0){
			if(j-1>=0){F(i-1,j-1,c+1);}
			else if(j+1<b){F(i-1,j+1,c+1);}
			else{F(i-1,j,c+1);}
		}
		else{
			if(j-1>=0){F(i,j-1,c+1);}
			else if(j+1<b){F(i,j+1,c+1);}
			else{printf("?");}
		}
	}
	if(S[c]=='I'&&SJOI[i][j]=='I'){n++;}
}
int main(){
	int c,d,e;
	scanf("%d%d",&a,&b);	
	getchar();
	for(int i=0; i<a; i++){
		for(int j=0; j<b; j++) {
			scanf("%c",&SJOI[i][j]);
		}
		getchar();
	}
	for(int i=0; i<a; i++){
		for(int j=0; j<b; j++) {
			if(SJOI[i][j]=='S'){
				F(i,j,0);
			}
		}
	}	
	printf("\n[%d]\n",n);
	return 0;
}
